<!DOCTYPE html>
<html  lang="fr">
<head>
{% block meta %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
    <meta name="description" content="Here is my work for the exercise 'Create my first website with Symfony'">
    <meta name="author" content="I'm Tuyetrinhvo, Web developer">
{% endblock %}
    <title>{% block title %}Snowtricks Website{% endblock %}</title>

    {% block stylesheets %}

        <!-- Bootstrap Core CSS -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap-theme.min.css" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap-theme.min.css.map" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.2/normalize.min.css" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.3.0/animate.min.css" rel="stylesheet" >
        <!-- Theme CSS -->
        <link href="{{ asset('/assets/css/mysnowtricks.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('/assets/css/responsive.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('/assets/css/iline-icons.css') }}" type="text/css" rel="stylesheet" />
        <!-- Custom Fonts -->
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css"/>
        <!-- Google Font -->
        <link href="http://fonts.googleapis.com/css?family=Lato:400,700" rel="stylesheet" type="text/css">
        <link href="http://fonts.googleapis.com/css?family=Allerta+Stencil:400,700,900:normal" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css"/>


    {% endblock %}

    {% block favicon %}
        <link rel="shortcut icon" type="image/x-icon" href="/assets/images/favicon.png" />
        <link rel="apple-touch-icon" href="/assets/images/apple-touch-icon.png"/>
    {% endblock %}

</head>

<body>

<!-- NAVIGATION START -->
<header class="fallone-navbar" data-id="default-navbar">
    <nav class="navbar navbar-default">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">
                    <img src="/assets/images/logo.png" alt="my logo" class="img-responsive" />
                </a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li><a  href="{{ path('ttv_website_homepage') }}">Accueil</a></li>
                    {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                     <li><a  href="{{ path('fos_user_security_logout') }}">DÃ©connexion</a></li>
                    <li><a  href="{{ path('ttv_website_add') }}">Ajouter une figure</a></li>
                    {% else %}
                     <li><a  href="{{ path('fos_user_security_login') }}">Connexion</a></li>
                     <li><a  href="{{ path('fos_user_registration_register') }}">Inscription</a></li>

                    {% endif %}
                </ul>

            </div><!-- /.navbar-collapse -->
        </div><!-- /.container -->
    </nav>
</header>
<!-- NAVIGATION END -->
<!-- MAIN CONTAINER -->

<div class="main-content">
    <!-- HEADER TREE -->
    <section class="header-tree">
        <!-- parallax background -->
        <div class="cover-1"></div>

    </section>
    <!-- HEADER TREE END -->

    <!-- BLOG BODY SECTION -->
    <section class="pattern-3 no-padding-right">
        <!-- .container -->
        <div class="container">
            <!-- .row -->
            <div class="row">
                <!-- .col-md-12 -->
                <div class="col-md-12">

                        {% block body %}

                        {% endblock %}

                </div>
            <!-- /.col-md-12 -->
            </div>
        <!-- /.row -->
        </div>
    <!-- /.container -->
    </section>
    <!-- BLOG BODY SECTION END -->
    <!-- BEGIN FOOTER -->
    <footer class="page-footer">
        <!-- FOOTER CONTENT -->
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <!-- .social -->
                    <ul class="social">
                        <li><a href="#" class="icon-xl iline3-facebook27"></a></li>
                        <li><a href="#" class="icon-xl iline3-instagram4"></a></li>
                        <li><a href="#" class="icon-xl iline3-twitter19"></a></li>
                        <li><a href="#" class="icon-xl iline3-linked3"></a></li>
                    </ul>
                    <!-- /.social -->
                </div>
            </div>
            <!-- /.row -->
            <div class="row">
                    <p class="col-md-12 text-center">&copy;  TuyetrinhVO {{ 'now'|date('Y') }} </p>
            </div>
            <!-- /.row copyright -->
        </div>
        <!-- /.container -->
        <!-- FOOTER CONTENT -->
    </footer>
    <!-- FOOTER END -->

</div>
<!-- /.main-content -->

<!-- MAIN CONTAINER END -->
<!-- Back to top -->
<div id="back-to-top" class="back-to-top">
    <a href="#" class="icon iline2-thin16 no-margin"></a>
</div>
<!-- Back to top end -->

{% block javascripts %}


    <!-- Theme JavaScript -->

 <script type="text/javascript" src="{{ asset('/assets/js/libs/jquery-1.11.2.min.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/jqBootstrapValidation.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/imagesloaded.pkgd.min.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/imagesloaded.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/jquery.magnific-popup.min.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/bootstrap/js/bootstrap.min.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/isotope.pkgd.min.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/ParallaxScrolling.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/jquery.mailchimp.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/wow.min.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/jquery.fittext.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/jquery.lettering.js') }}"></script>
 <script type="text/javascript" src="{{ asset('/assets/js/libs/jquery.textillate.js') }}"></script>



 <!--[if lt IE 9]>
 <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
 <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
 <![endif]-->

{% endblock %}

</body>
</html>